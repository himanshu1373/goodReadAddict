<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.goodReadsAddict" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
